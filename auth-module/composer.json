{
    "name": "johnjulius-21/auth-module",
    "description": "Reusable Laravel authentication module",
    "type": "library",
    "autoload": {
        "psr-4": {
            "AuthModule\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AuthModule\\AuthServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.31",
        "laravel/sanctum": "^3.0"
    }
}
